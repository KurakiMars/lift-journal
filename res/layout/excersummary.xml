<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" android:padding="5px"
  android:background="@color/sum_background"
  android:textColor="@color/sum_text">
    <LinearLayout android:id="@+id/linearLayout1" android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="vertical">
        <LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout2" android:focusableInTouchMode="false">
            <TextView android:id="@+id/textView1" android:text="ME Bench" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_width="wrap_content" android:layout_weight="0.5" android:capitalize="sentences"></TextView>
            <TextView android:id="@+id/textView2" android:text="PR" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_width="wrap_content" android:layout_weight="0.5" android:gravity="right" android:layout_gravity="center"></TextView>
        </LinearLayout>
        <TextView android:layout_width="fill_parent" android:id="@+id/textView3" android:textSize="28sp" android:textStyle="bold" android:textColor="@color/sum_text" android:layout_height="wrap_content" android:text="8/10/11" android:gravity="center" android:padding="4sp"></TextView>
        <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout3">
            <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/video" android:id="@+id/imageView1" android:gravity="left" android:layout_weight="0.5" android:scaleType="fitStart"></ImageView>
            <TextView android:layout_width="wrap_content" android:id="@+id/textView4" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:text="315lbs" android:layout_weight="0.5" android:gravity="right" android:layout_gravity="right"></TextView>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>
